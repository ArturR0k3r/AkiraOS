# ESP32-S3 Super Mini board configuration

# Copyright (c) 2026 AkiraOS
# SPDX-License-Identifier: Apache-2.0

config BOARD_ESP32S3_SUPERMINI
	select SOC_ESP32S3_MINI_N4R2
	select SOC_ESP32S3_PROCPU if BOARD_ESP32S3_SUPERMINI_ESP32S3_PROCPU
	select SOC_ESP32S3_APPCPU if BOARD_ESP32S3_SUPERMINI_ESP32S3_APPCPU
